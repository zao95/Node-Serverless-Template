{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": "maintained node versions",
                "useBuiltIns": "usage",
                "corejs": { "version": "3.19", "proposals": true }
            }
        ],
        "@babel/typescript"
    ],
    "plugins": [
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread"
    ]
}
